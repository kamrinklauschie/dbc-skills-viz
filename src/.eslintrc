{
  extends: "airbnb",
  env: {
    browser: true,
    jasmine: true,
  },
  settings: {
    "import/core-modules": [
      "react",
      "react-dom",
    ],
  },
  "rules": {
    "react/jsx-filename-extension": ["warn", { "extensions": [".js", ".jsx"] }],
    "react/prefer-stateless-function": ["off"]
  },
}
